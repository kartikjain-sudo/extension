{
    "manifest_version": 3,
    "name": "Timer",
    "version": "0.1.0",
    "description": "Save your time by analyzing your activity",
    "permissions": ["storage", "tabs", "activeTab", "notifications", "contextMenus", "alarms", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "48": "icons/stopwatch.png"
    },
    "action": {
      "default_icon": {
        "48": "icons/stopwatch.png"
      }
    }
}
